w$load <- function(h,...) {
w$load.dialog <- gwindow("Load Workspace",visible=FALSE)
w$loadg <-ggroup(horizontal=FALSE,container=w$load.dialog)
w$loadpath <- gfilebrowse("Select file",container=w$loadg)
# w$dataentry <- ggroup(horizontal=TRUE,container=w$importg)
# w$dataname <- gedit("newdata",container=w$dataentry)
# w$label <- glabel("Imported Dataset Name",container=w$dataentry)
w$load.go <- gbutton("Load",container=w$loadg,handler=w$load.do)
visible(w$load.dialog) <- TRUE
}
w$load()
w$load.do <- function(h,...) {
#TODO: append .RData if needed
path <- gsub("'","",svalue(w$loadpath))
load(file=path, envir=globalenv())
dispose(w$load.dialog)
}
w$load()
